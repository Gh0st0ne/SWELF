<form theme="dark">
  <label>SWELF</label>
  <fieldset submitButton="false" autoRun="true">
    <input type="dropdown" token="SourceComputer" searchWhenChanged="true">
      <label>SourceComputer</label>
      <fieldForLabel>SourceComputer</fieldForLabel>
      <fieldForValue>SourceComputer</fieldForValue>
      <search>
        <query>* SourceComputer="*" | dedup SourceComputer | table SourceComputer</query>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </search>
    </input>
  </fieldset>
  <row>
    <panel>
      <title>IP</title>
      <table>
        <title>IP</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | stats count by host</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">none</option>
        <format type="number" field="count">
          <option name="precision">0</option>
        </format>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>EventLogName</title>
      <table>
        <title>EventLogName</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$  | stats count by EventLogName| sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">false</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="totalsRow">false</option>
        <drilldown>
          <link target="_blank">search?q=sourcetype=SWELF%20$SourceComputer$%20%20%7C%20stats%20count%20by%20EventLogName%7C%20sort%20-%20count%20limit=10&amp;earliest=-7d@h&amp;latest=now</link>
        </drilldown>
      </table>
    </panel>
    <panel>
      <title>EventID</title>
      <chart>
        <title>EventID</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | table EventID | stats count by EventID</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
        <option name="height">389</option>
      </chart>
    </panel>
    <panel>
      <title>DisplayName</title>
      <chart>
        <title>DisplayName</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | table DisaplyName, ComputerName | stats count by DisplayName</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.chart">pie</option>
        <option name="charting.drilldown">none</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <title>Search_Rule Hits</title>
      <chart>
        <title>Search_Rule Hits</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | table Search_Rule, ComputerName | stats count by Search_Rule</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.axisY.abbreviation">auto</option>
        <option name="charting.axisY.scale">log</option>
        <option name="charting.chart">pie</option>
        <option name="charting.chart.showDataLabels">minmax</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="height">389</option>
      </chart>
    </panel>
    <panel>
      <title>Rulename TTPs</title>
      <table>
        <title>TTP Rulename</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$  rulename=* | stats count by rulename | sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Search_Rule Hits</title>
      <table>
        <title>Search_Rule Hits</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | table EventLogName, Search_Rule, EventID 
|stats count by  EventLogName, Search_Rule, EventID | sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">cell</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">true</option>
        <option name="wrap">true</option>
        <drilldown>
          <link target="_blank">search?q=sourcetype=SWELF%20$SourceComputer$%20%7C%20table%20EventLogName,%20Search_Rule,%20EventID%20%0A%7Cstats%20count%20by%20%20EventLogName,%20Search_Rule,%20EventID%20%7C%20sort%20-%20count%20limit=10&amp;earliest=-7d@h&amp;latest=now</link>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>ComputerName Logs Sent</title>
      <chart>
        <title>ComputerName Logs Sent</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ | table * | stats count by SourceComputer</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="charting.axisTitleX.visibility">visible</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.chart">bar</option>
        <option name="charting.drilldown">none</option>
        <option name="charting.legend.placement">right</option>
        <option name="refresh.display">progressbar</option>
      </chart>
    </panel>
    <panel>
      <table>
        <title>ERRORS</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ MethodInCode=* | table ComputerName,MethodInCode,Severity,LogSeverity | stats count by MethodInCode</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">cell</option>
        <drilldown>
          <link target="_blank">search?q=sourcetype=SWELF%20$SourceComputer$%20MethodInCode=*%20%7C%20table%20ComputerName,MethodInCode,Severity,LogSeverity%20%7C%20stats%20count%20by%20MethodInCode%7C%20sort%20-%20count%20limit=10&amp;earliest=-7d@h&amp;latest=now</link>
        </drilldown>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Top 10 Images using DNS</title>
      <table>
        <title>Images Using DNS</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ queryname=* image=*
| table queryname, image| stats count by queryname, image
| sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="percentagesRow">true</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">true</option>
        <drilldown>
          <link target="_blank">search?q=sourcetype=SWELF%20$SourceComputer$%20queryname=*%20image=*%0A%7C%20table%20queryname,%20image%7C%20stats%20count%20by%20queryname,%20image%0A%7C%20sort%20-%20count%20limit=10&amp;earliest=-24h@h&amp;latest=now</link>
        </drilldown>
      </table>
    </panel>
    <panel>
      <title>Top 10 Images using HTTP(s)</title>
      <table>
        <title>Images using HTTP(s)</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ image=* destinationportname=http* | table image,destinationport| stats count by image,destinationport
| sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">true</option>
        <option name="refresh.display">progressbar</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Rare Bottom 10 DNS Images</title>
      <table>
        <title>Rare Bottom 10 DNS Images</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ queryname=* image=*
| table queryname, image| stats count by queryname, image
| sort - count
| rare queryname,image limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">true</option>
        <option name="refresh.display">progressbar</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
    <panel>
      <title>Rare Bottom 10 HTTPs</title>
      <table>
        <title>Rare Bottom 10 HTTPs</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$ image=* destinationportname=http* 
| table image,destinationport
| stats count by image,destinationport
| sort - count
| rare image,destinationport limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">true</option>
        <option name="refresh.display">progressbar</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <title>Top 10 Parnet Child Processes</title>
      <table>
        <title>Parnet Child Processes</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$  image=* parentimage=* | table image,parentimage| stats count by image,parentimage
| sort - count limit=10</query>
          <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">true</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
    <panel>
      <title>Bottom 10 Parnet Child Processes</title>
      <table>
        <title>Parnet Child Processes</title>
        <search>
          <query>sourcetype=SWELF $SourceComputer$  image=* parentimage=* | table image,parentimage| stats count by image,parentimage
| sort - count 
| rare image,parentimage limit=10</query>
         <earliest>-7d@h</earliest>
          <latest>now</latest>
        </search>
        <option name="count">10</option>
        <option name="drilldown">row</option>
        <option name="percentagesRow">true</option>
        <option name="refresh.display">progressbar</option>
        <option name="rowNumbers">false</option>
        <option name="totalsRow">true</option>
      </table>
    </panel>
  </row>
</form>
